{
  "name": "Selenium Reservation Server",
  "version": "1.1",
  "slug": "local_selenium_reservation",
  "description": "Run Selenium reservation script",
  "startup": "services",
  "boot": "auto",
  "arch": ["amd64"],
  "map": ["config:rw"],
  "ports": {
    "5000/tcp": 5000
  },

  "options": {
    "email": "",
    "password": "",
    "tokens": [],
    "whitelist": [],
    "default_day": "Lu/Ma/Mi/Jo/Vi/Sa/Du",
    "default_hour": "10:00-21:00",
    "sport": "Baschet/Fotbal/Popice/Tenis/Tenis cu peretele/Tenis de masa",
    "location": "Manastur/Gheorgheni",
    "timeout": "10"
  },

  "schema": {
    "email": "str",
    "password": "str",
    "default_day": "str",
    "default_hour": "str",
    "sport": "str",
    "location": "str",
    "timeout": "str",
    
    "tokens": ["str?"],
    "whitelist": ["str?"]

  },

  "watchdog": "http://[HOST]:5000/health"
}
